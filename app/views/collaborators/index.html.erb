<h1><%= @wiki.title %> Collaborators </h1>
<table class="table table-striped">
  <thead>
    <th>Name</th>
    <th>Email</th>
    <th>Access</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to "Give Access", class: "btn btn-success" %></td>
    </tr>
    <% end %>
  </tbody>
</table>